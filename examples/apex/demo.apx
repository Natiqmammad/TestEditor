import nats;
import nats.btoi;
import nats.is_prime as prime;

fn weighted_score(value) {
    var score = nats.gcd(value, 192);
    score = score * 2;
    return score + nats.sum_digits(value);
}

fn apex() {
    let base = 270;
    let enriched = weighted_score(base);
    let bonus = btoi(prime(97));
    return enriched + bonus;
}
